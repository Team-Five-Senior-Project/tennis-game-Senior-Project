  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf public",
    "copy:images": "imagemin assets/img/* --out-dir=public/dist/img",
    "copy:js": "copyfiles -f node_modules/phaser/dist/phaser.min.js public/dist/js",
    "copy:pages": "copyfiles -f src/pages/index.html public",
    "js:min": "babel src/js --out-dir public/dist/js --presets=@babel/preset-env,babel-preset-minify",
    "js": "babel src/js --out-dir public/dist/js --presets=babel-preset-minify",
    "build": "npm run clean && npm run copy:images && npm run copy:js && npm run copy:pages && npm run js",
    "serve": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team-Five-Senior-Project/tennis-game-Senior-Project.git"
