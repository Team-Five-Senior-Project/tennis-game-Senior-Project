  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "copyfiles": "^2.4.1",
    "http-server": "^0.12.3",
    "imagemin-cli": "^6.0.0",
    "phaser": "^3.51.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
